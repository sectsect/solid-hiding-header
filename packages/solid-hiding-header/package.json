{
  "version": "2.0.9",
  "name": "@sect/solid-hiding-header",
  "description": "SolidJS header that disappears on scroll down and appears on scroll up.",
  "license": "MIT",
  "author": "sect",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sectsect/solid-hiding-header.git"
  },
  "homepage": "https://github.com/sectsect/solid-hiding-header#readme",
  "bugs": {
    "url": "https://github.com/sectsect/solid-hiding-header/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "solid": "./dist/source/index.jsx",
      "import": "./dist/esm/index.js",
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "dev": "vite serve dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "prepublishOnly": "pnpm build",
    "format": "prettier -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "update-deps": "taze -w && pnpm i",
    "lint": "eslint --ext .ts,tsx src",
    "lint:fix": "eslint --fix --ext .ts,tsx src",
    "type-check": "tsc --noEmit --pretty",
    "type-check:watch": "npm run type-check -- --watch",
    "release": "release-it"
  },
  "lint-staged": {
    "*": [
      "secretlint"
    ],
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "eslint"
    ],
    "src/**/*.{ts,tsx}": [
      "tsc-files --noEmit"
    ],
    "src/**/*.json": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "solid-js": ">=1.0.0"
  },
  "dependencies": {
    "hiding-header": "^0.9.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@secretlint/secretlint-rule-preset-recommend": "^7.0.7",
    "@solidjs/testing-library": "^0.8.4",
    "@testing-library/jest-dom": "^6.1.3",
    "@types/node": "^20.8.4",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@vitest/coverage-v8": "^0.34.6",
    "@vue/test-utils": "^2.4.1",
    "babel-preset-solid": "^1.8.0",
    "eslint": "8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-deprecation": "^2.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-solid": "^0.13.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-testing-library": "^6.0.2",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "prettier": "3.0.3",
    "rollup": "^4.0.2",
    "rollup-preset-solid": "^2.0.1",
    "secretlint": "^7.0.7",
    "solid-js": "^1.8.0",
    "taze": "^0.11.3",
    "tsc-files": "^1.1.4",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-plugin-solid": "^2.7.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  },
  "keywords": [
    "solid",
    "solidjs",
    "header",
    "scroll",
    "typescript"
  ],
  "packageManager": "pnpm@8.7.0"
}
